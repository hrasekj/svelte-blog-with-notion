{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 120,
  "tabWidth": 2,
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-organize-imports"],
  "organizeImportsSkipDestructiveCodeActions": true,
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
